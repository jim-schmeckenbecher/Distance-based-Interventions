@article{st.germain2012,
  title = {Direct and Indirect Forms of Non-Suicidal Self-Injury: Evidence for a Distinction},
  shorttitle = {Direct and Indirect Forms of Non-Suicidal Self-Injury},
  author = {St. Germain, Sarah A. and Hooley, Jill M.},
