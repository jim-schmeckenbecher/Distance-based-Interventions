  keywords = {Adolescent,Adult,Aftercare,Attempted,Cell Phone,China,Female,Humans,Male,Middle Aged,Patient Acceptance of Health Care,Patient Discharge,Patient Satisfaction,Pilot Projects,Secondary Prevention,Social Support,Suicide,Young Adult},
