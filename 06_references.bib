  author = {Eaddy, Michael and Zullo, Lucas and Horton, Sarah E. and Hughes, Jennifer L. and Kennard, Betsy and Diederich, Andrew and Emslie, Graham J. and Schuster, Lisa and Stewart, Sunita M.},
  date = {2019-08},
