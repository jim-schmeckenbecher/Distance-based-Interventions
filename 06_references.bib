  author = {Rücker, Gerta and Nikolakopoulou, Adriani and Papakonstantinou, Theodoros and Salanti, Georgia and Riley, Richard D. and Schwarzer, Guido},
  date = {2020-12},
  journaltitle = {BMC Medical Research Methodology},
  volume = {20},
