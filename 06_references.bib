  shorttitle = {Trait Liabilities and Specific Promotive Processes in Psychopathology},
  author = {Buchman-Schmitt, Jennifer M. and Brislin, Sarah J. and Venables, Noah C. and Joiner, Thomas E. and Patrick, Christopher J.},
  date = {2017},
  journaltitle = {Journal of Affective Disorders},
