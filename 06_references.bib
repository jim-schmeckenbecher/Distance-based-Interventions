@article{fernandez-castilla2020,
  title = {The Application of Meta-Analytic (Multi-Level) Models with Multiple Random Effects: A Systematic Review},
  shorttitle = {The Application of Meta-Analytic (Multi-Level) Models with Multiple Random Effects},
  author = {Fernández-Castilla, Belén and Jamshidi, Laleh and Declercq, Lies and Beretvas, S. Natasha and Onghena, Patrick and Van den Noortgate, Wim},
  date = {2020-03-11},
  journaltitle = {Behavior Research Methods},
  issn = {1554-3528},
  doi = {10.3758/s13428-020-01373-9},
  url = {http://link.springer.com/10.3758/s13428-020-01373-9},
  urldate = {2020-08-10},
  abstract = {In meta-analysis, study participants are nested within studies, leading to a multilevel data structure. The traditional random effects model can be considered as a model with a random study effect, but additional random effects can be added in order to account for dependent effects sizes within or across studies. The goal of this systematic review is three-fold. First, we will describe how multilevel models with multiple random effects (i.e., hierarchical three-, four-, five-level models and cross-classified random effects models) are applied in meta-analysis. Second, we will illustrate how in some specific three-level meta-analyses, a more sophisticated model could have been used to deal with additional dependencies in the data. Third and last, we will describe the distribution of the characteristics of multilevel meta-analyses (e.g., distribution of the number of outcomes across studies or which dependencies are typically modeled) so that future simulation studies can simulate more realistic conditions. Results showed that four- or five-level or cross-classified random effects models are not often used although they might account better for the meta-analytic data structure of the analyzed datasets. Also, we found that the simulation studies done on multilevel metaanalysis with multiple random factors could have used more realistic simulation factor conditions. The implications of these results are discussed, and further suggestions are given.},
  langid = {english},
  file = {/Users/jimschmeckenbecher/Zotero/storage/FPJ2UCII/Fernández-Castilla et al. - 2020 - The application of meta-analytic (multi-level) mod.pdf}
}

