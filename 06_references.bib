@article{comtois2019,
  title = {Effect of {{Augmenting Standard Care}} for {{Military Personnel With Brief Caring Text Messages}} for {{Suicide Prevention A Randomized Clinical Trial}}},
  author = {Comtois, Katherine Anne and Kerbrat, Amanda H. and DeCou, Christopher R. and Atkins, David C. and Majeres, Justine J. and Baker, Justin C. and Ries, Richard K.},
